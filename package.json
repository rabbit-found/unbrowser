{
  "name": "@unbrowser/core",
  "version": "0.1.0-alpha.1",
  "description": "Official SDK for Unbrowser cloud API - intelligent web browsing for AI agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
