    "type": "git",
    "url": "git+https://github.com/rabbit-found/unbrowser.git"
  },
  "devDependencies": {
    "@types/node": "^22.8.4",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
